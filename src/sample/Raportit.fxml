<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<BorderPane xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.HallintaController">
    <top>
        <HBox spacing="10.0" BorderPane.alignment="CENTER">
            <padding>
                <Insets bottom="5.0" left="5.0" right="5.0" top="10.0" />
            </padding>
            <children>
                <Label fx:id="hallintaLabel" text="VILLAGE NEWBIES OY">
                    <font>
                        <Font name="System Bold" size="29.0" />
                    </font>
                </Label>
                <Pane prefHeight="3.0" prefWidth="209.0" />
                <Button fx:id="mokkiButton" cancelButton="true" mnemonicParsing="false" onAction="#mokkiButtonOnAction" prefHeight="31.0" prefWidth="136.0" text="VARAUS" />
                <Button fx:id="asiakasButton" mnemonicParsing="false" onAction="#asiakasButtonOnAction" prefHeight="31.0" prefWidth="118.0" text="HALLINTA" />
                <Button fx:id="varausButton" mnemonicParsing="false" onAction="#varausButtonOnAction" prefHeight="31.0" prefWidth="118.0" text="RAPORTIT" />
            </children>
        </HBox>
    </top>
    <left>
        <VBox prefHeight="762.0" prefWidth="130.0" spacing="10.0" BorderPane.alignment="CENTER">
            <children>
                <ComboBox prefWidth="150.0" promptText="toiminta-alue" />
                <DatePicker promptText="saapumispäivä" />
                <DatePicker promptText="lähtöpäivä" />
                <Pane prefHeight="18.0" prefWidth="120.0" />
                <TextField promptText="min. hinta" />
                <TextField promptText="max. hinta" />
                <Pane prefHeight="14.0" prefWidth="120.0" />
                <TextField promptText="henkilömäärä" />
                <Pane prefHeight="18.0" prefWidth="120.0" />
                <Label text="varustelu:">
                    <font>
                        <Font name="System Bold" size="14.0" />
                    </font>
                </Label>
                <ComboBox prefWidth="150.0" />
                <CheckBox mnemonicParsing="false" text="Sauna" />
                <CheckBox mnemonicParsing="false" text="CheckBox" />
                <CheckBox mnemonicParsing="false" text="CheckBox" />
                <CheckBox mnemonicParsing="false" text="CheckBox" />
                <Label text="Palvelut:">
                    <font>
                        <Font size="14.0" />
                    </font>
                </Label>
                <CheckBox mnemonicParsing="false" text="CheckBox" />
                <CheckBox mnemonicParsing="false" text="CheckBox" />
                <CheckBox mnemonicParsing="false" text="CheckBox" />
            </children>
            <padding>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
            </padding>
        </VBox>
    </left>
    <center>
        <ScrollPane BorderPane.alignment="CENTER">
            <content>
                <VBox fx:id="tietoVBox" prefHeight="568.0" prefWidth="850.0" spacing="15.0" />
            </content>
        </ScrollPane>
    </center>
</BorderPane>
