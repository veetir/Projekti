<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<BorderPane fx:id="varausNaytto" prefHeight="768.0" prefWidth="1024.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.HallintaController">
    <top>
        <HBox prefHeight="53.0" prefWidth="1024.0" spacing="10.0" BorderPane.alignment="CENTER">
            <padding>
                <Insets bottom="5.0" left="5.0" right="5.0" top="10.0" />
            </padding>
            <children>
                <Label fx:id="hallintaLabel" text="VILLAGE NEWBIES OY">
                    <font>
                        <Font name="System Bold" size="29.0" />
                    </font>
                </Label>
                <Pane prefHeight="3.0" prefWidth="209.0">
               <children>
                  <Label layoutX="75.0" layoutY="10.0" text="Hallinta">
                     <font>
                        <Font size="18.0" />
                     </font></Label>
               </children></Pane>
            <Pane prefHeight="43.0" prefWidth="427.0">
               <children>
                      <Button fx:id="hallintaButton" layoutX="146.0" mnemonicParsing="false" onAction="#hallintaButtonOnAction" prefHeight="43.0" prefWidth="136.0" text="HALLINTA">
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor>
                  </Button>
                      <Button fx:id="varausButton" mnemonicParsing="false" onAction="#varausButtonOnAction" prefHeight="43.0" prefWidth="136.0" text="VARAUS">
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor>
                  </Button>
                      <Button fx:id="raportitButton" layoutX="291.0" mnemonicParsing="false" onAction="#raportitButtonOnAction" prefHeight="43.0" prefWidth="136.0" text="RAPORTIT">
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor>
                  </Button>
               </children>
            </Pane>
            </children>
        </HBox>
    </top>
   <center>
      <TabPane prefHeight="710.0" prefWidth="272.0" tabClosingPolicy="UNAVAILABLE" BorderPane.alignment="CENTER">
        <tabs>
          <Tab text="Mökit">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="681.0" prefWidth="261.0">
                     <children>
                        <BorderPane prefHeight="683.0" prefWidth="1024.0">
                           <left>
                              <VBox prefHeight="634.0" prefWidth="240.0" BorderPane.alignment="CENTER">
                                 <children>
                                    <ChoiceBox fx:id="alueValinta" prefWidth="200.0" />
                                 </children>
                              </VBox>
                           </left>
                           <center>
                              <ListView fx:id="mokkiLista" prefHeight="634.0" prefWidth="789.0" BorderPane.alignment="CENTER" />
                           </center>
                           <bottom>
                              <HBox alignment="CENTER" prefHeight="50.0" prefWidth="200.0" spacing="100.0" BorderPane.alignment="CENTER">
                                 <children>
                                    <Button fx:id="lisaaMokkiButton" mnemonicParsing="false" onAction="#lisaaMokkiButtonOnAction" text="Lisää" />
                                    <Button fx:id="muokkaaMokkiaButton" mnemonicParsing="false" onAction="#muokkaaMokkiaButtonOnAction" text="Muokkaa" />
                                    <Button fx:id="poistaMokkiButton" mnemonicParsing="false" onAction="#poistaMokkiButtonOnAction" text="Poista" />
                                 </children>
                              </HBox>
                           </bottom>
                        </BorderPane>
                     </children></AnchorPane>
            </content>
          </Tab>
          <Tab text="Asiakkaat">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <BorderPane prefHeight="683.0" prefWidth="1024.0">
                           <bottom>
                              <HBox alignment="CENTER" prefHeight="50.0" prefWidth="200.0" spacing="100.0" BorderPane.alignment="CENTER">
                                 <children>
                                    <Button fx:id="lisaaAsiakasButton" mnemonicParsing="false" onAction="#lisaaAsiakasButtonOnAction" text="Lisää" />
                                    <Button fx:id="muokkaaAsiakastaButton" mnemonicParsing="false" onAction="#muokkaaAsiakastaButtonOnAction" text="Muokkaa" />
                                    <Button fx:id="poistaAsiakasButton" mnemonicParsing="false" onAction="#poistaAsiakasButtonOnAction" text="Poista" />
                                 </children>
                              </HBox>
                           </bottom>
                           <center>
                              <ListView fx:id="asiakasLista" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
                           </center>
                        </BorderPane>
                     </children></AnchorPane>
            </content>
          </Tab>
            <Tab text="Varaukset">
              <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <BorderPane prefHeight="683.0" prefWidth="1024.0">
                           <left>
                              <VBox alignment="TOP_CENTER" prefHeight="633.0" prefWidth="186.0" spacing="25.0" BorderPane.alignment="CENTER">
                                 <children>
                                    <Label text="Rajaa" />
                                    <DatePicker fx:id="varausAlkuPvm" onAction="#varausAlkuPvmOnAction" promptText="Alkupvm." />
                                    <DatePicker fx:id="varausLoppuPvm" onAction="#varausLoppuPvmOnAction" promptText="Loppupvm." />
                                 </children>
                              </VBox>
                           </left>
                           <center>
                              <ListView prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
                           </center>
                           <bottom>
                              <HBox alignment="CENTER" prefHeight="50.0" prefWidth="200.0" spacing="100.0" BorderPane.alignment="CENTER">
                                 <children>
                                    <Button fx:id="muokkaaHallintaaButton" mnemonicParsing="false" onAction="#muokkaaHallintaaButtonOnAction" text="Muokkaa" />
                                    <Button fx:id="poistaVarausButton" mnemonicParsing="false" onAction="#poistaVarausButtonOnAction" text="Poista" />
                                 </children>
                              </HBox>
                           </bottom>
                        </BorderPane>
                     </children>
                  </AnchorPane>
              </content>
            </Tab>
            <Tab fx:id="toimAlueTab" onSelectionChanged="#toimAlueTabSelected" text="Toim.alue">
              <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <Label layoutX="362.0" layoutY="1.0" text="Toiminta-alueet">
                           <font>
                              <Font size="18.0" />
                           </font>
                        </Label>
                        <ScrollPane layoutX="49.0" layoutY="58.0" prefHeight="600.0" prefWidth="910.0">
                           <content>
                              <VBox fx:id="toimAlueVbox" prefHeight="600.0" prefWidth="895.0" />
                           </content>
                        </ScrollPane>
                        <Label layoutX="119.0" layoutY="40.0" text="ID">
                           <font>
                              <Font name="System Bold" size="14.0" />
                           </font></Label>
                        <Label layoutX="425.0" layoutY="40.0" text="Nimi">
                           <font>
                              <Font name="System Bold" size="14.0" />
                           </font></Label>
                        <Button fx:id="muokkaaToimButton" disable="true" layoutX="759.0" layoutY="16.0" mnemonicParsing="false" onAction="#muokkaaToimButtonOnAction" opacity="0.3" text="Muokkaa" />
                        <Button fx:id="poistaToimButton" disable="true" layoutX="833.0" layoutY="16.0" mnemonicParsing="false" onAction="#poistaToimButtonOnAction" opacity="0.3" text="Poista" />
                        <Button fx:id="lisaaUusiToimAlueButton" defaultButton="true" layoutX="907.0" layoutY="16.0" mnemonicParsing="false" onAction="#lisaaUusiToimAlueButton" text="Lisää" />
                     </children>
                  </AnchorPane>
              </content>
            </Tab>
        </tabs>
      </TabPane>
   </center>
</BorderPane>
